		<item>
	    	<linkId value="2.7.a" />
	    	<text value="2.7 Can only have up to 3 answers (using 'maxOccurs) " />
			<type value="text" />
		</item>
        <item>
            <!-- item/maxOccurs -->
            <extension url="http://hl7.org/fhir/StructureDefinition/questionnaire-maxOccurs">
                <valueInteger value="3"/> 
